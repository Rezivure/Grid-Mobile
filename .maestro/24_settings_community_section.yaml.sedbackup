appId: app.mygrid.grid
---

# Start with fresh login
- runFlow: flows/login_testuser1.yaml

# Test: Scroll to Community section in settings and verify links
# Prerequisite: testuser1 logged in on map screen

- launchApp
- waitForAnimationToEnd
- extendedWaitUntil:
    visible: "My Contacts"
    timeout: 10000

# Navigate to settings
- tapOn:
    point: "16,100"
- waitForAnimationToEnd
- assertVisible: "Settings"

# Scroll down to Community section
- swipe:
    start: "50%,70%"
    end: "50%,40%"
- waitForAnimationToEnd
- swipe:
    start: "50%,70%"
    end: "50%,40%"
- waitForAnimationToEnd

- assertVisible: "Community"

# Navigate back
- tapOn:
    point: "30,100"
- waitForAnimationToEnd
- extendedWaitUntil:
    visible: "My Contacts"
    timeout: 10000
