#include? "Pods/Target Support Files/Pods-Runner/Pods-Runner.release.xcconfig"
#include "Generated.xcconfig"

// Force load the vodozemac Rust library to ensure symbols are available
OTHER_LDFLAGS = $(inherited) -force_load ${PODS_CONFIGURATION_BUILD_DIR}/flutter_vodozemac/libvodozemac_bindings_dart.a

// Prevent stripping of vodozemac symbols in release builds
STRIP_STYLE = non-global
DEAD_CODE_STRIPPING = NO
GCC_GENERATE_DEBUGGING_SYMBOLS = YES
